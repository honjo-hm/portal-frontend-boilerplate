{
  "name": "@portal-frontend/server",
  "version": "0.0.1",
  "dependencies": {
    "@portal-frontend/shared": "^0.0.1"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p .",
    "watch": "../../node_modules/.bin/tsc -p . -w",
    "clean": "rm -rf .build",
    "reload": "../../node_modules/.bin/pm2 reload server",
    "server": "../../node_modules/.bin/pm2 start .build/server/server.js --watch server",
    "start:local": "NODE_ENV=local npm run clean; npm run build; ../../node_modules/.bin/concurrently \"npm run watch\" \"npm run server\""
  },
  "publishConfig": {
    "access": "public"
  }
}
